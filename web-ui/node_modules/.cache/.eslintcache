50px' }}\n          animate={{ y: 0, position: 'fixed' }}\n          exit={{ y: '150px', transition: {\n                type: \"tween\",\n                ease: [0.7, 0, 0.13, 1],\n                duration: 0.5,\n                delay: 0.1\n            } }}\n          transition={{\n              type: \"tween\",\n              ease: [0.7, 0, 0.13, 1],\n              duration: 0.5,\n              delay: 0.1\n          }}\n        >\n          <WorkName>{projectValue}</WorkName>\n          <TagsContainer>{tagsDisplayed}</TagsContainer>\n          <Details\n            to={\"/\" + projectValue + \"/works\"}\n            id={\"proj\" + projectValue}\n          >\n            Details  →\n          </Details>\n          {visit_link}\n        </WorkActions>\n\n        {projects_list}\n\n      </WorkListContainer>\n\n      <ContentBg\n        as={motion.div}\n        initial={{ opacity: 0 }}\n        animate={{ opacity: 1 }}\n        exit={{ opacity: 0 }}\n        transition={{\n            type: \"tween\",\n            ease: [0.7, 0, 0.13, 1],\n            duration: 0.8,\n        }}\n      >\n        <img src={rightImageSource}/>\n        <BgBlur />\n      </ContentBg>\n\n      <WorkImageContainer>\n        <WorkImage\n          as={motion.div}\n          initial={{ opacity: 0, scale: 0.99 }}\n          animate={{ opacity: 1, scale: 1}}\n          exit={{ opacity: 0, scale: 0.99, transition: {\n                type: \"tween\",\n                ease: [0.7, 0, 0.13, 1],\n                duration: 0.5,\n            } }}\n          transition={{\n              type: \"tween\",\n              ease: [0.7, 0, 0.13, 1],\n              duration: 0.5,\n          }}\n        >\n          <img src={rightImageSource}/>\n        </WorkImage>\n      </WorkImageContainer>\n    </Container>\n  );\n};\n\nexport default connect(({ projects }) => ({ projects }))(Blog);\n","/home/portfolio/v2/web-ui/src/components/ThemeToggle/index.js",["357","358","359"],"// import React, { useState, useEffec